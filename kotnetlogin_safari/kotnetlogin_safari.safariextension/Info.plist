<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Albert Jorissen</string>
	<key>Builder Version</key>
	<string>7534.45</string>
	<key>CFBundleDisplayName</key>
	<string>Kotnet Login</string>
	<key>CFBundleIdentifier</key>
	<string>kotnetloginextension</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>settings.html</string>
		<key>Toolbar Items</key>
		<array>
			<dict>
				<key>Identifier</key>
				<string>trigger</string>
				<key>Image</key>
				<string>Icon-16.png</string>
				<key>Label</key>
				<string>Trigger Kotnet Login</string>
			</dict>
		</array>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>login.js</string>
			</array>
			<key>Start</key>
			<array>
				<string>redirect.js</string>
				<string>error.js</string>
			</array>
		</dict>
	</dict>
	<key>Description</key>
	<string>Kotnet Login is a browser extension that provides automatic login for websites of the KULeuven</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>*.kuleuven.be</string>
				<string>*.groept.be</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://kotnetloginextension.googlecode.com/hg/kotnetlogin_safari/updates.plist</string>
	<key>Website</key>
	<string>http://kotnetloginextension.googlecode.com</string>
</dict>
</plist>
